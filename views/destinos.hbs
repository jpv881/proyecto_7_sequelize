<section id="sectionInicio">
    <div class="wrapper">
        <div class="container-fluid">
            <div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img class="d-block w-100" src="../images/comienzo.jpg" alt="salida">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="../images/vuelo.jpg" alt="vuelo">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="../images/destino.jpg" alt="destino">
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section id="sectionDestinos">
    <div class="wrapper">
        <div class="container-fluid">
            <div class="row">
                {{#if travels.length}}
                    {{#each travels}}
                        <div class="col-md-3">
                            <div class="card" style="width: 18rem;">
                                <img class="card-img-top" src="..." alt="Card image cap">
                                <div class="card-body">
                                    <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                                </div>
                            </div>
                        </div>
                    {{/each}}
                {{/if}}
            </div>
        </div>
    </div>
</section>
<section id="sectionQuienes">
    <div class="wrapper">
        <div class="container-fluid">

        </div>
    </div>
</section>